rsync -avz --rsync-path="sudo rsync" evrypi@192.168.1.61:/lib/ sysroot/lib
rsync -avz --rsync-path="sudo rsync" evrypi@192.168.1.61:/usr/lib/ sysroot/usr/lib
rsync -avz --rsync-path="sudo rsync" evrypi@192.168.1.61:/opt/vc/ sysroot/opt/vc
  
  
